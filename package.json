{
  "name": "scss-symbols-parser",
  "version": "1.1.3",
  "description": "A very simple and fast SCSS Symbols parser",
  "license": "MIT",
  "repository": "mrmlnc/scss-symbols-parser",
  "author": {
    "name": "Denis Malinochkin",
    "url": "canonium.com"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "main": "out/index.js",
  "typings": "out/index.d.ts",
  "files": [
    "out/index.d.ts",
    "out/index.js",
    "out/tokenizer.js",
    "out/tokenizer.d.ts"
  ],
  "keywords": [
    "scss",
    "parser",
    "symbols",
    "variables",
    "mixins",
    "imports"
  ],
  "devDependencies": {
    "bootstrap": "^4.3.1",
    "mocha": "^3.4.2",
    "rimraf": "^2.6.1",
    "tslint": "^5.4.3",
    "tslint-config-xo": "^1.2.0",
    "typescript": "^2.3.4"
  },
  "scripts": {
    "clean": "rimraf out",
    "lint": "tslint \"src/**/*.ts\" -p . -t stylish",
    "compile": "tsc",
    "test": "mocha \"out/**/*.spec.js\" -s 0",
    "build": "npm run clean && npm run lint && npm run compile && npm test",
    "watch": "npm run clean && npm run lint && npm run compile -- --watch"
  }
